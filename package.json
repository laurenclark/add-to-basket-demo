{
    "name": "add-to-basket-demo",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "fetch-schema": "npx apollo schema:download --endpoint=https://vitl-hasura.herokuapp.com/v1/graphql graphql-schema.json",
        "generate-types": "npx apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql --outputFlat",
        "clean-generated": "find . -name '__generated__' -type d -prune -exec rm -rf '{}' +",
        "lint": "next lint"
    },
    "dependencies": {
        "@apollo/client": "^3.4.15",
        "graphql": "^15.6.0",
        "next": "11.1.2",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-loading-icons": "^1.0.6",
        "styled-components": "^5.3.1"
    },
    "devDependencies": {
        "@types/graphql": "^14.5.0",
        "@types/react": "17.0.25",
        "@types/styled-components": "^5.1.14",
        "apollo": "^2.33.6",
        "babel-plugin-styled-components": "^1.13.2",
        "eslint": "7.32.0",
        "eslint-config-next": "11.1.2",
        "typescript": "4.4.3"
    }
}
